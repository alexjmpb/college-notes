{
    "emeraldwalk.runonsave": {
       "commands": [
            {
                "match": "^(?!setup\\.tex$|boxes\\.tex$).*\\.tex$",
                "cmd": "bash ${workspaceFolder}/sync_util/main.sh ${workspaceFolder} ${fileDirname} ${relativeFile} ${file}"
            }
       ]
    },
    "latex-workshop.latex.outDir": "%DIR%/build/",
    "latex-workshop.latex.autoBuild.run": "never"
}